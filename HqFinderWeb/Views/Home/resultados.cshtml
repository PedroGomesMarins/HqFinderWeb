
@{
    ViewBag.Title = "Resultados";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/resultados.css" rel="stylesheet" />
    <title>mostrarResultados</title>
</head>
<body>
    <div>

        <h2>Resultados</h2>
        <h4>Foram encontrados @ViewBag.Resultados.Count resultados</h4>
        <ul>
            @foreach (Resultado res in ViewBag.Resultados)
        {

            <li>
                @res.preco
                <a>@res.link</a>
            </li>
    }
        </ul>

    </div>
</body>
</html>
